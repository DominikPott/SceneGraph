# lookdev node attributes
[group Node Attributes]
    [attr docstring]
        default             STRING    "Look development mapping node. Maps models to shaders."
        label               STRING    "Doc"
        desc                STRING    "node docstring."

[group Asset]

    [input model]
        default            NODE       ""
        label              STRING     "Model"

    [input shader_mapping]
        default            NODE       ""
        label              STRING     "Mapping"
        desc               STRING     "shaders mapping file."

    [input shader]
        default            NODE       ""
        label              STRING     "Shaders"
        desc               STRING     "shaders file."

    [input texture]
        default            NODE       ""
        label              STRING     "Textures"
