#!/usr/bin/env python
import os
import sys
from PySide import QtGui, QtCore



if __name__ == "__main__":
    from SceneGraph import scenegraph
    app = QtGui.QApplication(sys.argv)
    app.setStyle("oxygen")
    #app.setStyle("gtk") # gtk is default
    sgui = scenegraph.SceneGraphUI()
    sgui.show()
    sys.exit(app.exec_())